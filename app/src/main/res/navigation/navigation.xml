<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation"
    app:startDestination="@id/splash">
    <fragment
        android:id="@+id/splash"
        android:name="com.dscepointblank.pointblank.activities.Splash"
        android:label="Splash" >
        <action
            android:id="@+id/action_splash_to_viewpager"
            app:destination="@id/viewpager"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_splash_to_notificatonactivity"
            app:destination="@id/notificatonactivity"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/viewpager"
        android:name="com.dscepointblank.pointblank.activities.Viewpager"
        android:label="Viewpager" >
        <action
            android:id="@+id/action_viewpager_to_login2"
            app:destination="@id/login2"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/login2"
        android:name="com.dscepointblank.pointblank.activities.Login"
        android:label="Login" >
        <action
            android:id="@+id/action_login2_to_signup"
            app:destination="@id/signup"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>
    <fragment
        android:id="@+id/signup"
        android:name="com.dscepointblank.pointblank.activities.Signup"
        android:label="Signup" />
    <fragment
        android:id="@+id/notificatonactivity"
        android:name="com.dscepointblank.pointblank.activities.Notificatonactivity"
        android:label="Notificatonactivity" />
</navigation>